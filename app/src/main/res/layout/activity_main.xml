<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Job"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/bgnewfull">



    <TextView
        android:id="@+id/consultant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/aclonica"
        android:freezesText="false"
        android:gravity="center"
        android:text=" Appointment"
        android:textColor="@color/white"
        android:textSize="@dimen/my_dimen2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/name"
        android:layout_width="@dimen/my_dimen4"
        android:layout_height="@dimen/my_dimen5"
        android:layout_marginTop="@dimen/my_dimen6"
        android:layout_marginEnd="@dimen/my_dimen6"
        android:background="@drawable/border"
        android:backgroundTint="?android:attr/textColorHintInverse"
        android:ems="10"
        android:fontFamily="@font/acme"
        android:freezesText="false"
        android:gravity="center"
        android:hint="@string/CONNAME"
        android:inputType="textPersonName"
        android:minHeight="@dimen/my_dimen7"
        android:textColor="@color/white"
        android:textSize="@dimen/my_dimen8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/consultant"
        tools:ignore="DuplicateIds" />


    <EditText
        android:id="@+id/number"
        android:layout_width="@dimen/my_dimen4"
        android:layout_height="@dimen/my_dimen5"
        android:layout_marginStart="@dimen/my_dimen10"
        android:layout_marginEnd="@dimen/my_dimen6"
        android:background="@drawable/border"
        android:backgroundTint="?android:attr/textColorHintInverse"
        android:ems="10"
        android:fontFamily="@font/acme"
        android:freezesText="false"
        android:gravity="center"
        android:hint="@string/CONMOBILE"
        android:inputType="textMultiLine"
        android:minHeight="@dimen/my_dimen7"
        android:textColor="@color/white"
        android:textSize="@dimen/my_dimen8"
        app:layout_constraintBottom_toTopOf="@+id/guideline3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.916"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/consultant"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/age"
        android:layout_width="@dimen/my_dimen4"
        android:layout_height="@dimen/my_dimen5"
        android:layout_marginStart="@dimen/my_dimen9"
        android:layout_marginEnd="@dimen/my_dimen6"
        android:background="@drawable/border"
        android:backgroundTint="?android:attr/textColorHintInverse"
        android:ems="10"
        android:fontFamily="@font/acme"
        android:freezesText="false"
        android:gravity="center"
        android:hint="@string/CONAGE"
        android:inputType="number"
        android:textColor="@color/white"
        android:textSize="@dimen/my_dimen8"
        app:layout_constraintBottom_toTopOf="@+id/guideline3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/number"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/address"
        android:layout_width="@dimen/my_dimen4"
        android:layout_height="@dimen/my_dimen5"
        android:layout_marginEnd="@dimen/my_dimen6"
        android:background="@drawable/border"
        android:backgroundTint="?android:attr/textColorHintInverse"
        android:ems="10"
        android:fontFamily="@font/acme"
        android:freezesText="false"
        android:gravity="center"
        android:hint="@string/CONADDR"
        android:inputType="textPersonName"
        android:minHeight="@dimen/my_dimen7"
        android:textColor="@color/white"
        android:textSize="@dimen/my_dimen8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline3"
        tools:ignore="MissingConstraints" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="@dimen/my_dimen4"
        android:layout_height="@dimen/my_dimen5"
        android:background="@drawable/border"
        android:backgroundTint="?android:attr/textColorHintInverse"
        android:contextClickable="true"

        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline3" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/acme"
        android:text="Select your date"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/spinner"
        app:layout_constraintEnd_toEndOf="@+id/spinner"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="@+id/spinner"
        app:layout_constraintTop_toBottomOf="@+id/address" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="89dp"
        android:layout_height="53dp"
        app:layout_constraintBottom_toTopOf="@+id/button9"
        app:layout_constraintEnd_toEndOf="@+id/spinner"
        app:layout_constraintHorizontal_bias="0.992"
        app:layout_constraintStart_toStartOf="@+id/spinner"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/down" />

    <Button
        android:id="@+id/button9"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/btncolor"
        android:fontFamily="@font/acme"
        android:text="SUBMIT"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        app:cornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.582"
        app:layout_constraintStart_toEndOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/spinner" />

    <Button
        android:id="@+id/button7"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@color/btncolor"
        android:fontFamily="@font/acme"
        android:text="@string/cancel"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        app:cornerRadius="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner" />


    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayoutCompat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        app:srcCompat="@drawable/down" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/linearLayoutCompat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:layout_marginRight="20dp"
        android:gravity="center|bottom"
        android:orientation="horizontal"
        android:weightSum="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline2">

        <ImageView
            android:id="@+id/homeBtn"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="@dimen/my_dimen3"
            android:layout_marginBottom="@dimen/my_dimen3"
            android:layout_weight="0.2"
            app:srcCompat="@drawable/unselected_home"
            app:tint="@color/black" />

        <ImageView
            android:layout_marginLeft="20dp"
            android:id="@+id/searchBtn"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginBottom="@dimen/my_dimen3"
            android:layout_weight="0.2"
            app:srcCompat="@drawable/unselected_applyjob"
            app:tint="@color/black" />

        <ImageView
            android:id="@+id/addBtn"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="@dimen/my_dimen3"
            android:layout_marginBottom="@dimen/my_dimen3"
            android:layout_weight="0.2"
            app:srcCompat="@drawable/unselected_addjob"
            app:tint="@color/black" />


        <ImageView
            android:id="@+id/viewBtn"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="28dp"
            android:layout_marginBottom="32dp"
            android:layout_weight="0.2"
            app:srcCompat="@drawable/unselected_helpdesk"
            app:tint="@color/black" />

        <ImageView
            android:id="@+id/updateBtn"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="28dp"
            android:layout_marginBottom="32dp"
            android:layout_weight="0.2"
            app:srcCompat="@drawable/unselected_account"
            app:tint="@color/black" />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="615dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="312dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="584dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="467dp" />

</androidx.constraintlayout.widget.ConstraintLayout>